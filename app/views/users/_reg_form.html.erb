<%= form_for @user do |f| %>
	<%= f.label "Username" %>
	<%= f.text_field :name %><br>
	<%= f.label "e-mail" %>
	<%= f.text_field :email %><br>
	<%= f.label "Password" %>
	<%= f.password_field :password %><br>
	<%= f.label "Confirm your password" %>
	<%= f.password_field :password_confirmation %><br>
	<%= f.submit "Create" %>
<% end %>